{
  "name": "lazy-ass",
  "description": "Lazy assertions without performance penalty",
  "version": "0.0.0-semantic-release",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": {
    "url": "https://github.com/bahmutov/lazy-ass/issues"
  },
  "config": {
    "next-update": {
      "skip": [
        "grunt",
        "grunt-cli",
        "karma"
      ]
    },
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm test"
      ],
      "pre-push": [
        "npm run size"
      ],
      "post-commit": [],
      "post-merge": []
    }
  },
  "contributors": [],
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/condition-travis": "5.0.2",
    "bad-line": "0.1.1",
    "condition-node-version": "1.3.0",
    "coveralls": "2.13.3",
    "expect.js": "0.3.1",
    "git-issues": "1.3.1",
    "github-post-release": "1.13.1",
    "grunt": "0.4.5",
    "grunt-banner": "0.6.0",
    "grunt-clean-console": "0.1.1",
    "grunt-cli": "1.0.0-rc1",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-jshint": "1.1.0",
    "grunt-contrib-uglify": "3.3.0",
    "grunt-contrib-watch": "1.0.1",
    "grunt-deps-ok": "0.9.0",
    "grunt-gh-pages": "2.0.0",
    "grunt-karma": "0.11.2",
    "grunt-mocha-test": "0.13.3",
    "grunt-nice-package": "0.10.4",
    "grunt-npm2bower-sync": "0.9.1",
    "jshint-stylish": "2.2.1",
    "karma": "0.12.32",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.4",
    "matchdep": "1.0.1",
    "mocha": "3.5.3",
    "next-update-travis": "1.7.1",
    "phantomjs": "2.1.7",
    "phantomjs-prebuilt": "2.1.16",
    "pkgfiles": "2.3.2",
    "pre-git": "3.17.1",
    "semantic-release": "6.3.6",
    "simple-commit-message": "3.3.2",
    "time-grunt": "1.4.0"
  },
  "engines": {
    "node": "> 0.8"
  },
  "files": [
    "index.js",
    "index.html",
    "bower.json",
    "test/demo.js",
    "index.d.ts"
  ],
  "homepage": "https://github.com/bahmutov/lazy-ass",
  "keywords": [
    "assertion",
    "assertions",
    "browser",
    "debugging",
    "defensive",
    "lazy",
    "node"
  ],
  "license": "MIT",
  "main": "index.js",
  "release": {
    "verifyConditions": [
      {
        "path": "@semantic-release/condition-travis"
      },
      {
        "path": "condition-node-version",
        "node": "4.2.2"
      }
    ],
    "generateNotes": "github-post-release",
    "analyzeCommits": "simple-commit-message"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/lazy-ass.git"
  },
  "scripts": {
    "build": "grunt",
    "commit": "git-issues && commit-wizard",
    "coveralls": "cat coverage/PhantomJS*/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "demo": "grunt gh-pages",
    "dont-break": "dont-break --timeout 30",
    "issues": "git-issues",
    "mocha": "mocha test/*.spec.js",
    "pkgfiles": "pkgfiles",
    "pretest": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "size": "tarball=\"$(npm pack .)\"; wc -c \"${tarball}\"; tar tvf \"${tarball}\"; rm \"${tarball}\";",
    "test": "grunt test",
    "watch": "grunt watch"
  },
  "types": "index.d.ts"
}
